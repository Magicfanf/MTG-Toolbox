    function setRatingResponse(ratingResponse) {
        form.elements['form-rating'].value = ratingResponse;
    }

    var availableCardsNumber = 64;
    
    var card1SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
    img1.src = dynamicUrl(card1SetNo);
    form.elements['form-SetNo1'].value = correspondenceTable(card1SetNo); //card1SetNo
    
    if(dynamicUrlCardB(card1SetNo)=""){
        img1b.style = "display:none";
    } else {
        img1b.src = dynamicUrlCardB(card1SetNo);
        img1b.style = "display:inline";
    }    
    
    var card2SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
    img2.src = dynamicUrl(card2SetNo);
    form.elements['form-SetNo2'].value = correspondenceTable(card2SetNo); //card2SetNo

    if(dynamicUrlCardB(card2SetNo)=""){
        img2b.style = "display:none";
    } else {
        img2b.src = dynamicUrlCardB(card2SetNo);
        img2b.style = "display:inline";
    }
    
    form.addEventListener('submit', e => {
        e.preventDefault()
        fetch(scriptURL, {
                method: 'POST',
                body: new FormData(form)
            })
            .then(response => console.log('Success!', response))
            .catch(error => console.error('Error!', error.message))

        card1SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
        img1.src = dynamicUrl(card1SetNo);
        form.elements['form-SetNo1'].value = correspondenceTable(card1SetNo); //card1SetNo

        if(dynamicUrlCardB(card1SetNo)=""){
            img1b.style = "display:none";
        } else {
            img1b.src = dynamicUrlCardB(card1SetNo);
            img1b.style = "display:inline";
        }   
        
        card2SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
        img2.src = dynamicUrl(card2SetNo);
        form.elements['form-SetNo2'].value = correspondenceTable(card2SetNo); //card2SetNo

        if(dynamicUrlCardB(card2SetNo)=""){
            img2b.style = "display:none";
        } else {
            img2b.src = dynamicUrlCardB(card2SetNo);
            img2b.style = "display:inline";
        }   
    })
    
