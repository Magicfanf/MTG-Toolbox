    function dynamicUrlCardB(cardSetNo) {
        var url
        switch (cardSetNo) {
            case 15: url = "https://docs.google.com/uc?export=view&id=1Sbh0-5LOa8zJC194mlUSilqiFIu-g5OL"; break;
