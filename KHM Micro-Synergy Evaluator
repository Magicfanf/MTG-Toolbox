    function dynamicUrlCardB(cardSetNo) {
        var url
        switch (cardSetNo) {
            case 15: url = "https://docs.google.com/uc?export=view&id=1SJEm6bLd460FtP5F1vA3rMgrkAuEEs1J"; break;
            case 40: url = "https://docs.google.com/uc?export=view&id=1QeY7-sS43RfVQHfu9lLfHtnDn-vBLaCa"; break;
            case 112: url = "https://docs.google.com/uc?export=view&id=1dSc2z52zTcgu4OpvyWSUvXGXA-GIqEmE"; break;
            case 114: url = "https://docs.google.com/uc?export=view&id=1d2II3qjFYDRsq4cjHdzNGcR-LXRKASPf"; break;
            case 168: url = "https://docs.google.com/uc?export=view&id=1mUxihOlit8Ycsx2XaCF2gRV0AIn_Byh_"; break;
            case 181: url = "https://docs.google.com/uc?export=view&id=1gPpOG2V94nVAAqrgMicILPjqhPHIY79q"; break;
            default: url = "";
        }
        return url;
    }

    function setRatingResponse(ratingResponse) {
        form.elements['form-rating'].value = ratingResponse;
    }

    var availableCardsNumber = 72;
    
    var card1SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
    img1.src = dynamicUrl(card1SetNo);
    form.elements['form-SetNo1'].value = correspondenceTable(card1SetNo); //card1SetNo
    
    if(dynamicUrlCardB(correspondenceTable(card1SetNo))==""){
        img1b.style = "display:none";
    } else {
        img1b.src = dynamicUrlCardB(correspondenceTable(card1SetNo));
        img1b.style = "display:inline";
    }
    
    var card2SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
    img2.src = dynamicUrl(card2SetNo);
    form.elements['form-SetNo2'].value = correspondenceTable(card2SetNo); //card2SetNo

    if(dynamicUrlCardB(correspondenceTable(card2SetNo))==""){
        img2b.style = "display:none";
    } else {
        img2b.src = dynamicUrlCardB(correspondenceTable(card2SetNo));
        img2b.style = "display:inline";
    }
    
    form.addEventListener('submit', e => {
        e.preventDefault()
        fetch(scriptURL, {
                method: 'POST',
                body: new FormData(form)
            })
            .then(response => console.log('Success!', response))
            .catch(error => console.error('Error!', error.message))

        card1SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
        img1.src = dynamicUrl(card1SetNo);
        form.elements['form-SetNo1'].value = correspondenceTable(card1SetNo); //card1SetNo

        if(dynamicUrlCardB(correspondenceTable(card1SetNo))==""){
            img1b.style = "display:none";
        } else {
            img1b.src = dynamicUrlCardB(correspondenceTable(card1SetNo));
            img1b.style = "display:inline";
        }
        
        card2SetNo = Math.floor(Math.random() * availableCardsNumber) + 1;
        img2.src = dynamicUrl(card2SetNo);
        form.elements['form-SetNo2'].value = correspondenceTable(card2SetNo); //card2SetNo

        if(dynamicUrlCardB(correspondenceTable(card2SetNo))==""){
            img2b.style = "display:none";
        } else {
            img2b.src = dynamicUrlCardB(correspondenceTable(card2SetNo));
            img2b.style = "display:inline";
        }
    })
    
